<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.karm.Karm">
    <method name="version">
      <arg type="s" direction="out"/>
    </method>
    <method name="taskIdFromName">
      <arg type="s" direction="out"/>
      <arg name="taskName" type="s" direction="in"/>
    </method>
    <method name="addTask">
      <arg type="i" direction="out"/>
      <arg name="taskName" type="s" direction="in"/>
    </method>
    <method name="setPerCentComplete">
      <arg type="s" direction="out"/>
      <arg name="taskName" type="s" direction="in"/>
      <arg name="perCent" type="i" direction="in"/>
    </method>
    <method name="bookTime">
      <arg type="i" direction="out"/>
      <arg name="taskId" type="s" direction="in"/>
      <arg name="iso8601StartDateTime" type="s" direction="in"/>
      <arg name="durationInMinutes" type="i" direction="in"/>
    </method>
    <method name="getError">
      <arg type="s" direction="out"/>
      <arg name="karmErrorNumber" type="i" direction="in"/>
    </method>
    <method name="totalMinutesForTaskId">
      <arg type="i" direction="out"/>
      <arg name="taskId" type="s" direction="in"/>
    </method>
    <method name="starttimerfor">
      <arg type="s" direction="out"/>
      <arg name="taskname" type="s" direction="in"/>
    </method>
    <method name="stoptimerfor">
      <arg type="s" direction="out"/>
      <arg name="taskname" type="s" direction="in"/>
    </method>
    <method name="stopalltimers">
      <arg type="s" direction="out"/>
    </method>
    <method name="deletetodo">
      <arg type="s" direction="out"/>
    </method>
    <method name="setpromptdelete">
      <arg type="s" direction="out"/>
      <arg name="prompt" type="b" direction="out"/>
    </method>
    <method name="getpromptdelete">
      <arg type="b" direction="out"/>
    </method>
    <method name="exportcsvfile">
      <arg type="s" direction="out"/>
      <arg name="filename" type="s" direction="in"/>
      <arg name="from" type="s" direction="in"/>
      <arg name="to" type="s" direction="in"/>
      <arg name="type" type="i" direction="in"/>
      <arg name="decimalMinutes" type="b" direction="in"/>
      <arg name="allTasks" type="b" direction="in"/>
      <arg name="delimiter" type="s" direction="in"/>
      <arg name="quote" type="s" direction="in"/>
    </method>
    <method name="importplannerfile">
      <arg type="s" direction="out"/>
      <arg name="filename" type="s" direction="in"/>
    </method>
    <method name="save">
      <arg type="b" direction="out"/>
    </method>
    <method name="quit" />
  </interface>
</node>
