{
  "app-id": "org.kde.ktimetracker",
  "runtime": "org.kde.Platform",
  "runtime-version": "6.9",
  "sdk": "org.kde.Sdk",
  "command": "ktimetracker",
  "rename-icon": "ktimetracker",
  "finish-args": [
    "--share=ipc",
    "--device=dri",
    "--socket=wayland",
    "--socket=fallback-x11",
    "--talk-name=org.kde.StatusNotifierWatcher"
  ],
  "cleanup": [
    "*.a",
    "*.la",
    "/doc",
    "/docs",
    "/include",
    "/lib/cmake",
    "/lib/pkgconfig",
    "/lib/x86_64-linux-gnu/cmake",
    "/lib/x86_64-linux-gnu/pkgconfig",
    "/man",
    "/share/cmake",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig"
  ],
  "modules": [
    {
      "name": "ktimetracker",
      "buildsystem": "cmake-ninja",
      "sources": [
        {
          "type": "dir",
          "path": "."
        }
      ]
    }
  ]
}
